function applyFilters(keyword, country, age_from, age_to, gender, education){
  age_from = parseInt(age_from);
  age_to = parseInt(age_to);
  if(age_from < 10 || age_from > 100)
    return;
  if(age_to < 10 || age_to > 100)
    return;
  if(isNaN(age_from))
    age_from = ""
  if(isNaN(age_to))
    age_to = ""
  $.ajax({
    url: "<%= search_with_filters_path %>",
    type: "post",
    data: { search: keyword, country: country, age_from: age_from, 
      age_to: age_to, gender: gender, education: education },
    error:function(){
      // display error message to user after merging with report story
    }   
  }); 
}
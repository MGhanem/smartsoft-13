<% a = Vote.get_unvoted_keywords(current_or_guest_gamer.id, params[:count].to_i, params[:lang].to_i) %>

<% if a.count == 0 %>
	wordsInDb = false;
	<% a = Vote.get_unvoted_keywords(-1, params[:count].to_i, params[:lang].to_i) %>
<% else %>
	wordsInDb = true;
	wasPrompted = false;
<% end %>
	

wordsArray = [];
wordExistsInArray = [];
<% a.each do |k| %>
	wordsArray.push("<%= k.name %>");
<% end %>

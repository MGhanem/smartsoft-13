<h1>Projects</h1>
<br />
<a class="btn" href="/projects/new"><i class="icon-plus"></i> New Project</a>
<br />
<% @projects.each do |project| %>
  <div class="boxed">
  Name:
  <br />
  <%= project.name %>
  <br />
  <% project.developers_shared.each do |dev|%>
  <%= dev.name %>
   <a href="/projects/remove_developer_from_project?dev_id=<%=dev.id%>&project_id=<%=project.id%>">Remove Share</a>
  <%end%>
  Categories:
  <br />
  <% project.categories.each do |c| %>
    <%= c.name %>
  <% end %>
  <br />
  Description:
  <br />	
  <%= project.description %>
</div>
<br />
<% end %>